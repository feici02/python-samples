    "# get method can provide a default value\n",
    "# if the key doesn't exist.\n",
    "\n",
    "users = [ {'name': 'Jason', 'type': 'admin'}, {'name': 'Judy'} ]\n",
    "\n",
    "for user in users:\n",
    "    print('The type of {} is {}.'.format(user.get('name'), user.get('type', 'user')))"
